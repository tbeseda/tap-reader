{
  "name": "tap-reader",
  "version": "0.0.10",
  "description": "A smol, streaming TAP parser.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/tap-reader.git"
  },
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "tap-reader": "src/index.js"
  },
  "files": [
    "src/"
  ],
  "scripts": {
    "lint": "eslint --ext .mjs --ext .js src/ test/",
    "lint.fix": "npm run lint -- --fix",
    "start": "tape test/tap/make-tap.js | node examples/table-reporter.js",
    "tape": "tape test/**/*-test.js",
    "test": "npm run tape"
  },
  "author": "tbeseda",
  "license": "Apache-2.0",
  "devDependencies": {
    "cli-table3": "^0.6.3",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "tape": "^5.7.2",
    "vendurl": "^0.1.1"
  },
  "vendurl": {
    "destination": "src/vendor",
    "packages": {
      "yaml.js": "yaml@2.3.2?target=es2021"
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "ignorePatterns": [
      "src/vendor"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  },
  "keywords": [
    "tap",
    "parser",
    "testing",
    "tape"
  ]
}

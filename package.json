{
  "name": "tap-reader",
  "version": "0.2.0",
  "author": "tbeseda",
  "license": "Apache-2.0",
  "description": "A smol, streaming TAP parser.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/tap-reader.git"
  },
  "main": "src/index.js",
  "bin": {
    "tap-reader": "src/index.js"
  },
  "files": ["src/"],
  "scripts": {
    "vendurl": "vendurl",
    "lint": "biome ci",
    "lint.fix": "biome check --write",
    "start": "tape test/tap/make-tap.js | node examples/table-reporter.js",
    "tape": "tape test/**/*-test.js",
    "test": "npm run lint && npm run tape"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "cli-table3": "^0.6.5",
    "tape": "^5.8.1",
    "vendurl": "^0.3.0"
  },
  "vendurl": {
    "destination": "src/vendor",
    "packages": {
      "yaml.js": "yaml@2.3.2?target=es2021"
    }
  },
  "keywords": ["tap", "parser", "testing", "tape"]
}
